    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            // "program": "${workspaceFolder}/build/modules/modules_test",// viewer
            "program": "${workspaceFolder}/build/bin/civview_test",// viewer
            "args": [],

            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",// this is necessary for the thirdparty
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}